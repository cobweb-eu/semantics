# baseURI: http://prophet.ucd.ie/ontology/cobweb/map

@prefix :        <http://prophet.ucd.ie/ontology/cobweb/map#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

:Configuration
      rdf:type rdfs:Class .

:Constant
      rdf:type rdfs:Class .

:Mapping
      rdf:type rdfs:Class .

:ValueMap
      rdf:type rdfs:Class ;
      rdfs:subClassOf :Mapping .

:forValue
      rdf:type rdf:Property ;
      rdfs:domain :ValueMap .

:hasTransformation
      rdf:type rdf:Property ;
      rdfs:domain :Mapping ;
      rdfs:range xsd:string .

:key  rdf:type rdf:Property ;
      rdfs:domain :Constant ;
      rdfs:range xsd:string .

:mapType
      rdf:type rdf:Property ;
      rdfs:domain :Mapping ;
      rdfs:range xsd:string .

:mappedValue
      rdf:type rdf:Property ;
      rdfs:domain :ValueMap .

:mappings
      rdf:type rdf:Property ;
      rdfs:domain :Configuration ;
      rdfs:range :Constant , :Mapping .

:mapsToElement
      rdf:type rdf:Property .

:templateFile
      rdf:type rdf:Property ;
      rdfs:domain :Configuration ;
      rdfs:range xsd:anyURI .

:type
      rdf:type rdf:Property ;
      rdfs:domain :Configuration ;
      rdfs:range xsd:string .

:value
      rdf:type rdf:Property ;
      rdfs:domain :Constant ;
      rdfs:range xsd:string .

<http://prophet.ucd.ie/ontology/cobweb/security#map>
      rdf:type owl:Ontology ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .
